     sed -i.bk 's/ *☼☀ (/,☼☀(/g' shira_scln_pattika1.2.5.txt
     sed -i.bk 's/\([ ,☀☼]\)\([A-Za-z][A-Za-z]\)\([^\n ,\.;☼☀]*\) *☼☀ *\2/\1\2\3,☼☀\2/Ig' shira_scln_pattika1.2.5.txt
