     sed -i.bk 's/ \([a-zA-Z]\)\([^,\.;☀☼✦ \n]*\)\([,\.;☀☼✦][,\.;☀☼✦]*\)\([^,\.;☀☼✦ -\n]*\)-\1/-\1\2\3\4-\1/g' ./shira_scln_pattika1.2.5.txt
     sed -i.bk 's/ tam\| tavAn\| zika\| lika\| ti\| tri\| nti\| trika\| taH\| sika\| lbika\| yika\| tsikA\| dikA\| nvikA/Q#q&/Ig' ./shira_scln_pattika1.2.5.txt
